<mapper namespace="com.ync.project.front.mapper.InquiryMapper">

	<select id="getList"
		resultType="com.ync.project.domain.InquiryVO">
	<![CDATA[
		SELECT	*
		FROM	teamainquiry
		WHERE	inquiry_id > 0 
		]]>
	</select>
	<insert id="insert">
		INSERT INTO teamainquiry (			
									inquiry_id,
									title,
									reg_date,
									comment_date, 
									member_email,
									content,
									comments
								 )
								 VALUES (
								 	seq_reply.nextval,
									#{title}, 
									sysdate,
									sysdate, 
									#{member_email},  
									#{content}, 
									#{comments})
	</insert> 

