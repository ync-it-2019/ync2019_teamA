	<select id="read" resultType="com.ync.project.domain.InquiryVO">
	SELECT	
			inquiry_id,
			title,
			reg_date,
			comment_date,
			member_email,
			content,
			comments
	FROM	teamainquiry
	WHERE	inquiry_id = #{inquriy_id} 
	</select>


	<delete id="delete">
		DELETE	teamainquiry
		WHERE	inquiry_id = #{inquiry_id}
	</delete>
	
