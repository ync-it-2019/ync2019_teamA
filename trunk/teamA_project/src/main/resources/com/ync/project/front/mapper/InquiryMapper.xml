<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ync.project.front.mapper.InquiryMapper">

	<select id="getList"
		resultType="com.ync.project.domain.InquiryVO">
	<![CDATA[
		SELECT	*
		FROM	teamainquiry
		WHERE	inquiry_id > 0 
		]]>
	</select>
	<insert id="insert">
		INSERT INTO teamainquiry (			
									inquiry_id,
									title,
									reg_date,
									userid,
									content
								 )
								 VALUES (
								 	seq_reply.nextval,
									#{title}, 
									sysdate,
									#{userid},  
									#{content}
									)
	</insert> 


	<select id="read" resultType="com.ync.project.domain.InquiryVO">
	SELECT	
			inquiry_id,
			title,
			reg_date,
			comment_date,
			member_email,
			content,
			comments
	FROM	teamainquiry
	WHERE	inquiry_id = #{inquriy_id} 
	</select>


	<delete id="delete">
		DELETE	teamainquiry
		WHERE	inquiry_id = #{inquiry_id}
	</delete>
</mapper>